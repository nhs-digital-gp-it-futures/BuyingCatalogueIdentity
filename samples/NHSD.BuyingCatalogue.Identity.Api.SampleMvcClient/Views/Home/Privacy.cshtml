@{
    ViewData["Title"] = "Congratulations!";
}
<h1>@ViewData["Title"]</h1>

<p>You are successfully logged in</p>
<p id="sampleResourceResult">@ViewBag.Response</p>

<br/>

@if (User.Identity?.IsAuthenticated ?? false)
{
    <table>
        @foreach (var claim in User.Claims)
        {
            <tr>
                <td>@claim.Type</td>
                <td>@claim.Value</td>
            </tr>
        }
    </table>
}
